{
  "name": "@decisively-io/types-interview",
  "version": "0.2.0-alpha.6",
  "description": "A type package for the Decisively interview system.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc -w",
    "transpileToDist": "rm -rf dist && tsc",
    "prepublishOnly": "npm run check && yarn transpileToDist",
    "check:fix": "tsc --noEmit --project tsconfig.json && npx @biomejs/biome check --apply-unsafe .",
    "check": "tsc --noEmit --project tsconfig.json && npx @biomejs/biome check ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "decisively",
    "interview",
    "types",
    "typescript"
  ],
  "author": "Decisively",
  "license": "UNLICENSED",
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "typescript": "^4.9.5"
  }
}
